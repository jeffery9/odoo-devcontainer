<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <record id="group_library_manager" model="res.groups">
            <field name="name">Library Manager</field>
            <field name="comment">Administrator for Library Application</field>
            <field name="category_id" ref="base.module_category_administration"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]" />
        </record>

        <record id="group_library_librarian" model="res.groups">
            <field name="name">Library librarian</field>
            <field name="comment">Librarian for Library Application</field>
            <field name="category_id" ref="base.module_category_administration"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]" />
        </record>

        <record id="group_library_member" model="res.groups">
            <field name="name">Member librarian</field>
            <field name="comment">Member for Library Application</field>
            <field name="category_id" ref="base.module_category_administration"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]" />
        </record> 

    </data>
</odoo>